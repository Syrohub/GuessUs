{
  "name": "guessus",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:family": "cp configs/family/.env.family .env && vite",
    "dev:adult": "cp configs/adult/.env.adult .env && vite",
    "build": "vite build",
    "build:family": "./scripts/build-variant.sh family",
    "build:adult": "./scripts/build-variant.sh adult",
    "archive:family": "./scripts/build-variant.sh family --archive",
    "archive:adult": "./scripts/build-variant.sh adult --archive",
    "archive:all": "./scripts/build-variant.sh family --archive && ./scripts/build-variant.sh adult --archive",
    "preview": "vite preview",
    "cap:sync": "npx cap sync",
    "cap:copy": "npx cap copy",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "update:ios": "npm run build && npx cap sync ios",
    "update:ios:open": "npm run build && npx cap sync ios && npx cap open ios",
    "update:ios:family": "npm run build:family && npx cap open ios",
    "update:ios:adult": "npm run build:adult && npx cap open ios",
    "deploy:family": "npm run build:family && cd ios/App && fastlane family",
    "deploy:adult": "npm run build:adult && cd ios/App && fastlane adult",
    "deploy:all": "./scripts/deploy-all.sh"
  },
  "dependencies": {
    "@capacitor-firebase/analytics": "^7.5.0",
    "@capacitor-firebase/crashlytics": "^7.5.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/haptics": "^7.0.3",
    "@capacitor/ios": "^7.4.4",
    "@capgo/native-audio": "^7.11.2",
    "classnames": "^2.3.2",
    "cordova-plugin-purchase": "^13.12.1",
    "firebase": "^11.10.0",
    "lucide-react": "^0.294.0",
    "mobile-drag-drop": "^2.3.0-rc.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.9.3",
    "vite": "^5.0.8"
  }
}
